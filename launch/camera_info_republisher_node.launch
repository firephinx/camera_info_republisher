<launch>
 
  <node pkg="camera_info_republisher" type="camera_info_republisher_node" name="camera_info_republisher_node" output="screen">
    <param name="publish_frequency"           		value="10" />
    <param name="static_camera_info"          		value="true" />
    <param name="input_camera_info_topic_name"    	value="/camera/camera_info" />
    <param name="output_camera_info_topic_name"     value="/camera_info_republisher/camera/camera_info" />
    <param name="output_camera_info_frame_id_name"  value="" />
  </node>

</launch>
